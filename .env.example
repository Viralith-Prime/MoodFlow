# MoodFlow Environment Variables
# Copy to .env.local for local development

# Vercel KV Database (FREE TIER)
# Get these from Vercel Dashboard > Storage > KV
KV_REST_API_URL=your_kv_rest_api_url_here
KV_REST_API_TOKEN=your_kv_rest_api_token_here
KV_REST_API_READ_ONLY_TOKEN=your_kv_read_only_token_here

# App configuration
VITE_APP_NAME=MoodFlow
VITE_APP_VERSION=1.0.0
VITE_API_BASE_URL=https://your-app-name.vercel.app/api

# Geolocation API (optional - falls back to browser geolocation)
VITE_GEOLOCATION_API_KEY=your_geolocation_api_key_here

# Map tiles configuration (optional - defaults to OpenStreetMap)
VITE_MAP_TILES_URL=https://tile.openstreetmap.org/{z}/{x}/{y}.png
VITE_MAP_ATTRIBUTION=Â© OpenStreetMap contributors

# Analytics endpoint (future feature)
VITE_ANALYTICS_ENDPOINT=your_analytics_endpoint_here